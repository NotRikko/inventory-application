<!DOCTYPE html>
<html>
  <head>
    <title>TFT Inventory</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <% include navbar %>
    <div class="main">
      <h1>Origins</h1>
      <% if(origin_list.length) { %>
        <ul class="bodyList">
          <% origin_list.forEach((origin) => { %>
            <li><a href ="<%= origin.url %>"><%= origin.name %></a></li>
          <% }) %>
        </ul>
      <% } %>
      <p><a href="/origin/create">Add Origin</a></p>
    </div>
  </body>
</html>
