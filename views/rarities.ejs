<!DOCTYPE html>
<html>
  <head>
    <title>TFT Inventory</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <% include navbar %>
    <div class="main">
      <h1>Rarities</h1>
      <% if(rarity_list.length) { %>
        <ul class="bodyList">
          <% rarity_list.forEach((rarity) => { %>
            <li><a href ="<%= rarity.url %>"><%= rarity.name %></a></li>
          <% }) %>
        </ul>
      <% } %>
    </div>
  </body>
</html>
