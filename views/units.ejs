<!DOCTYPE html>
<html>
  <head>
    <title>TFT Inventory</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <% include navbar %>
    <div class="main">
      <h1>Units</h1>
      <% if(unit_list.length) { %>
        <ul class="bodyList">
          <% unit_list.forEach((unit) => { %>
            <li class="unitBox">
              <image src="<%= unit.image %>"></image>
              <a href ="<%= unit.url %>"><%= unit.name %></a>
            </li>
          <% }) %>
        </ul>
      <% } %>
    </div>
  </body>
</html>
